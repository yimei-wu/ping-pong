<!DOCTYPE html>
<html {{ site.language_attributes }}>
    <head>
        <meta charset="UTF-8">
        <meta name="viewport"
              content="width=device-width, initial-scale=1.0">
        <link rel="stylesheet" href="{{ theme.link }}/img/coreui-icons-master/css/all.min.css">
        <link rel='stylesheet' href='https://cdn-uicons.flaticon.com/2.2.0/uicons-regular-rounded/css/uicons-regular-rounded.css'>
        <link rel="stylesheet" href="{{ theme.link }}/scss/style.css" />
       
        {{ function("wp_head") }}
    </head>
    <body class="{{ body_class }}"
          data-template="base.twig">
        {{ function("wp_body_open") }}
        <main>
            {% block content %}
            {% endblock content %}
        </main>
        {% include "footer.twig" %}
        {{ function("wp_footer") }}
         <script src="{{ theme.link }}/scripts/app.js"></script>
    </body>
</html>
